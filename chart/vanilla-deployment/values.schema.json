                "memory": {
                    "title": "Server Memory",
                    "form": true,
                    "type": "object",
                    "properties": {
                        "initial": {
                            "title": "Initial Memory",
                            "description": "The inital amount of memory in megabytes.",
                            "form": true,
                            "type": "integer",
                            "render": "slider",
                            "sliderMin": 500,
                            "sliderMax": 2400,
                            "sliderStep": 100,
                            "sliderUnit": "M"
