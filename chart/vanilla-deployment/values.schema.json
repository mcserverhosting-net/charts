                        "buffer": {
                            "title": "buffer",
                            "form": true,
                            "type": "integer",
                            "render": "slider",
                            "sliderMin": 500,
                            "sliderMax": 2400,
                            "sliderStep": 100,
                            "sliderUnit": "M",
